{
  "private": true,
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2"
  },
  "lint-staged": {
    "server/**/*.py": [
      "uv run --project server ruff check --fix",
      "uv run --project server black"
    ],
    "front/**/*.{js,jsx,ts,tsx}": [
      "pnpm --dir front exec eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky"
  },
  "packageManager": "pnpm@10.14.0"
}
